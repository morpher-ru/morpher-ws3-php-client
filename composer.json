{
    "name": "morpher/ws3-client",
    "version": "0.2.1",
    "description": "Склонение по падежам на русском, украинском и казахском языках, расстановка ударений в текстах - PHP-клиент для API morpher.ru",
    "keywords": [
        "declension",
        "inflection",
        "pluralization",
        "russian",
        "ukrainian",
        "qazaq"
    ],
    "homepage": "https://github.com/morpher-ru/morpher-ws3-php-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Lukovnikov"
        }
    ],
    "require": {
        "php": "^7.4.0|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0",
        "rector/rector": "^0.14.8"
    },
    "autoload": {
        "psr-4": {
        },
        "classmap": [
            "src"
        ]
    },
    "config": {
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}